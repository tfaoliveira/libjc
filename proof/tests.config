[default]
bin  = easycrypt
args = -timeout 30 -max-provers 2 -p Z3 -p Alt-Ergo -I Jasmin:eclib -I crypto_core/keccakf160064bits

[test-chacha20]
okdirs = !crypto_stream/chacha20

[test-poly1305]
okdirs = !crypto_onetimeauth/poly1305

[test-keccakf160064bits]
okdirs = !crypto_core/keccakf160064bits

[test-keccak1600]
okdirs = !crypto_hash/keccak1600
