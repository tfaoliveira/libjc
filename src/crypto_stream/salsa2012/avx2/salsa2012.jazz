#ifndef CRYPTO_STREAM_SALSA2012_AVX2
#define CRYPTO_STREAM_SALSA2012_AVX2

param int SALSA20_ROUNDS=12;
//require "crypto_stream/salsa20/avx2/salsa20.jinc"
require "../../salsa20/avx2/salsa20.jinc"

#ifdef EXPORT

export fn salsa2012_avx2(reg u64 output plain len nonce key) -> reg u64
{
  reg u64 r;
  __salsa20_avx2(output, plain, len, nonce, key);
  _,_,_,_,_,r = #set0();
  return r;
}

#endif
#endif

