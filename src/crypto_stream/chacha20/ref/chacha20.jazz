#ifndef CRYPTO_STREAM_CHACHA20_REF
#define CRYPTO_STREAM_CHACHA20_REF

param int CHACHA_ROUNDS=20;
//require "crypto_stream/chacha/ref/chacha.jinc"
require "../../chacha/ref/chacha.jinc"

#ifdef EXPORT

export fn chacha20_ref(reg u64 output plain len nonce key) -> reg u64
{
  reg u64 r;
  __chacha_ref(output, plain, len, nonce, key);
  _,_,_,_,_,r = #set0();
  return r;
}

#endif
#endif

