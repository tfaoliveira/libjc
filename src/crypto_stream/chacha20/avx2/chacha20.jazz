#ifndef CRYPTO_STREAM_CHACHA20_AVX2
#define CRYPTO_STREAM_CHACHA20_AVX2

param int CHACHA_ROUNDS=20;
//require "crypto_stream/chacha/avx2/chacha.jinc"
require "../../chacha/avx2/chacha.jinc"

#ifdef EXPORT

export fn chacha20_avx2(reg u64 output plain len nonce key) -> reg u64
{
  reg u64 r;
  __chacha_avx2(output, plain, len, nonce, key);
  _,_,_,_,_,r = #set0();
  return r;
}

#endif
#endif

