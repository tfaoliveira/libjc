#ifndef X25519_CSWAP
#define X25519_CSWAP

fn _fe64_cswap(stack u64[4] xs, stack u64[4] ys, reg u64 swap) -> stack u64[4], stack u64[4]
{
  inline int i;
  reg u64[4] x y;
  reg u64 t mask;

  mask = 0;
  mask -= swap;

  x = xs;
  y = ys;

  for i=0 to 4
  {
    t  = x[i];
    t ^= y[i];
    t &= mask;

    x[i] ^= t; // ^ (x[i] ^ y[i]) if swap == 1 
    y[i] ^= t;
  }

  xs = x;
  ys = y;

  return xs, ys;
}

#endif
