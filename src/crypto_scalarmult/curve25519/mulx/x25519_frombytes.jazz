#ifndef X25519_FROMBYTES
#define X25519_FROMBYTES

fn _fe64_frombytes(reg u64 _f) -> stack u64[4], stack u64[4]
{
  inline int i;
  reg   u64[4] f;
  stack u64[4] f1s f2s;

  for i=0 to 3
  { f[i]   = [_f + 8*i];
    f1s[i] = f[i];
    f2s[i] = f[i]; }

  f[3]    = [_f + 8*3];
  f[3]   &= 0x7fffffffffffffff;
  f1s[3]  = f[3];
  f2s[3]  = f[3];
  
  return f1s, f2s;
}

#endif
