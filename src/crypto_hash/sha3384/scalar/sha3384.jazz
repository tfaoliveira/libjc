#ifndef CRYPTO_HASH_SHA3384_SCALAR
#define CRYPTO_HASH_SHA3384_SCALAR

#include "crypto_hash/keccak1600/scalar/keccak1600.jazz"

inline fn __sha3384_scalar(reg u64 out in inlen)
{
  reg u64 outlen rate;
  reg u8 trail_byte;

  outlen = (384/8);
  trail_byte = 0x6;
  rate = (832/8);

  __keccak1600_scalar(out, outlen, in, inlen, trail_byte, rate);
}

#ifdef EXPORT

export fn sha3384_scalar(reg u64 out in inlen)
{
  __sha3384_scalar(out, in, inlen);
}

#endif

#endif
