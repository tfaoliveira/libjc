#include "crypto_hash2curve/expand_message/xmd/sha256/ref0c/xmd.jazz"

export fn expand_message_xmd_sha256_ref0c(reg u64 out outlen _param ctx)
{
  reg u64 msg msglen DST DSTlen;

  msg    = (u64)[_param + 8*0];
  msglen = (u64)[_param + 8*1];
  DST    = (u64)[_param + 8*2];
  DSTlen = (u64)[_param + 8*3];

  _expand_message_xmd_sha256_ref0c(out, outlen, msg, msglen, DST, DSTlen, ctx);
}
