#include "crypto_core/keccakf160064bits/scalar/keccakf1600.jazz"

export fn keccakf1600_scalar(reg u64 out in _iotas)
{
  inline int i;
  stack u64[25] state;
  reg u64 t;

  for i=0 to 25
  { t = [in + 8*i];
    state[i] = t; }

  state = __keccakf1600_scalar(state);

  for i=0 to 25
  { t = state[i];
    [out + 8*i] = t; }
}
