fn __keccak_f1600_ref(stack u64[25] state) -> stack u64[25] {
  inline int round;
  stack u64[24] constants;
  constants = keccakRoundConstants();
  for round = 0 to 24 {
    state = keccakP1600_round(state, constants[round]);
  }
  return state;
}
